{
    "node": true,
    "browser": true,
    "es5": false,
    "esnext": true,
    "bitwise": false,
    "camelcase": false,
    "curly": false,
    "eqeqeq": true,
    "esnext": true,
    "immed": true,
    "latedef": true,
